		  <div class="field">
		    <%= f.label :session_name %><br>
		    <p><%= @film_session.session_name %></p>
		  </div>
		  <div class="field">
		    <%= f.label :place_number %><br>
		    <p><%= i %></p>
		  </div>
		  <div class="field">
		    <%= f.label :status %><br>
		    <%= f.check_box :status, class:"form form-control" %>
		  </div>
		  <br/>
		  <div class="actions">
		    <%= f.submit "Booking", :class => "btn btn-primary" %>
