<% if @array_of_places.present? %>
	<% @array_of_places.each do |n| %>
		<div class="col-md-6 booking_blocks" id="<%= n %>">
			<button class="close" id="close" value="<%= n %>">Ã—</button>
			<div class="container">
				<div class="row">
					<%= form_for Place.new do |f| %>
						<%= render partial: "shared/place", locals: {f: f, n: n, film_session: @film_session} %>
					<% end %>
				</div>
			</div>
		</div>
	<% end %>
	<div class="actions">
	    <%= submit_tag "Booking", :class => "btn btn-primary" %>
	</div>
<% end %>
