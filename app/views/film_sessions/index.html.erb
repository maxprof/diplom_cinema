<p id="notice"><%= notice %></p>

<h1>Listing Film Sessions</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Cinema</th>
      <th>Session name</th>
      <th>Number of session place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @film_sessions.each do |film_session| %>
      <tr>
        <td><%= film_session.cinema_id %></td>
        <td><%= film_session.session_name %></td>
        <td><%= film_session.number_of_session_place %></td>
        <td><%= link_to 'Show', film_session %></td>
        <td><%= link_to 'Edit', edit_film_session_path(film_session) %></td>
        <td><%= link_to 'Destroy', film_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

